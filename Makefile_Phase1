CFLAGS= -g -Wall -Werror
CC=clang

all: read_elf_header

read_elf_header.o : read_elf_header.c modules/freadoctet.h
		$(CC) -c $(CFLAGS) read_elf_header.c modules/freadoctet.h

read_elf_header : read_elf_header.o modules/freadoctet.o
		$(CC) $(CFLAGS) $^ -o $@  -lm

modules/freadoctet.o: modules/freadoctet.c modules/freadoctet.h

.PHONY: clean
clean:
		rm -f read_elf_header read_elf_header.o modules/freadoctet.o modules/freadoctet.h.gch
